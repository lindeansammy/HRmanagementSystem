<Window x:Class="WpfTeamProject.View.AdminRegister"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTeamProject.View"
        mc:Ignorable="d"
        Title="AdminRegister" Height="450" Width="800">
    <Grid Margin="0,0,2,0">
        <Label Content="User name：" HorizontalAlignment="Left" Margin="46,20,0,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="46,71,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="AdminUN" HorizontalAlignment="Left" Height="23" Margin="203,24,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="244"/>
        <TextBox x:Name="AdminPW" HorizontalAlignment="Left" Height="23" Margin="203,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="244"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="203,173,0,0" VerticalAlignment="Top" Width="92" RenderTransformOrigin="0.527,-1.647" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="372,173,0,0" VerticalAlignment="Top" Width="75" Click="btn_cancel"/>

    </Grid>
</Window>
